 r←init_selenium_htmlrenderer nul;z;arg
 {}⎕SE.UCMD'output.find on -i'
⍝ Initialize Selenium with Chrome driver
 path←∊1 ⎕NPARTS ##.TESTSOURCE,'../'    ⍝ set up global variable with path to the Selenium folder (will also define #.S as ref to Selenium later)
 arg←⎕NS''
 arg.folder←path
 arg.batch←1
 :If ~⊃r←⎕SE.Cider.OpenProject ⎕SE.Cider.CreateOpenParms arg    ⍝ open project using Cider
     ∘∘∘
     r←2⊃r
     →0
 :EndIf

 #.S←#.Selenium
 :If ⊃r←#.S.InitBrowser('HtmlRenderer','.'~⍨4↑2⊃'.'⎕WG'aplversion')80   ⍝ if there was an error initialising the browser
     ∘∘∘
     r←2⊃r                           ⍝ report it...
 :Else
     r←''
 :EndIf
